        <scene sceneID="pAa-Wg-K8H">
            <objects>
                <viewController id="Upc-UN-48T" userLabel="Hunt Seat Equitation" customClass="SectionIVC" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="9gb-J9-3VV">
                        <rect key="frame" x="0.0" y="0.0" width="320" height="416"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMaxY="YES"/>
                        <subviews>
                            <scrollView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleAspectFit" alwaysBounceVertical="YES" showsHorizontalScrollIndicator="NO" maximumZoomScale="0.0" id="759-I1-zRY">
